{
    "routes": [
      {
        "src": "/assets/(.*)", 
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*)\\.(js|css|svg|png|jpg|jpeg|gif|webp|ico|woff|woff2|ttf|otf|eot|ttc|mp4|webm|wav|mp3|ogg|m4a|aac|opus)$",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  